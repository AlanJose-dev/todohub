{
    "name": "alanjose/todohub",
    "description": "A simple MVC application to manage tasks;",
    "type": "project",
    "require": {
        "vlucas/phpdotenv": "^5.6",
        "robmorgan/phinx": "^0.16.5",
        "symfony/routing": "^7.2@beta",
        "symfony/http-foundation": "^7.2@beta",
        "guzzlehttp/guzzle": "^7.9",
        "monolog/monolog": "^3.8",
        "ext-apcu": "*",
        "symfony/validator": "^7.2@RC",
        "nesbot/carbon": "^3.8",
        "filp/whoops": "^2.16"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.4",
        "fakerphp/faker": "^1.24"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "helpers/functions.php"
        ],
        "classmap": [
            "app/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Alan Jos√©",
            "email": "alanjsdelima@gmail.com",
            "role": "Founder/Developer"
        }
    ],
    "minimum-stability": "alpha"
}
